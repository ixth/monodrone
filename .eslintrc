{
    "root": true,
    "parser": "babel-eslint",
    "extends": ["airbnb", "plugin:prettier/recommended", "prettier/react"],
    "plugins": ["babel"],
    "env": {
        "browser": true,
        "es6": true
    },
    "rules": {
        "import/no-extraneous-dependencies": 0,
        "no-underscore-dangle": 0
    },
    "settings": {
        "import/resolver": {
            "webpack": {
                "config": "./webpack.config.js"
            }
        }
    }
}
